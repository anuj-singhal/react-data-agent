{
  "table_name": "uae_banks_financial_data",
  "description": "Financial performance metrics for UAE banks including profits, deposits, and key ratios",
  "ddl": "CREATE TABLE uae_banks_financial_data (\n  bank VARCHAR2(100) PRIMARY KEY,\n  ytd_profit NUMBER(15,2),\n  deposits NUMBER(15,2),\n  nim NUMBER(5,2),\n  cost_income NUMBER(5,2),\n  npl_ratio NUMBER(5,2),\n  assets NUMBER(15,2),\n  loans NUMBER(15,2),\n  equity NUMBER(15,2)\n)",
  "columns": {
    "bank": {
      "type": "VARCHAR2(100)",
      "description": "Bank name or identifier",
      "nullable": false,
      "is_primary_key": true
    },
    "ytd_profit": {
      "type": "NUMBER(15,2)",
      "description": "Year-to-date profit in millions AED",
      "nullable": true,
      "unit": "millions AED"
    },
    "deposits": {
      "type": "NUMBER(15,2)",
      "description": "Total customer deposits",
      "nullable": true,
      "unit": "millions AED"
    },
    "nim": {
      "type": "NUMBER(5,2)",
      "description": "Net Interest Margin percentage",
      "nullable": true,
      "unit": "percentage"
    },
    "cost_income": {
      "type": "NUMBER(5,2)",
      "description": "Cost to Income ratio",
      "nullable": true,
      "unit": "percentage"
    },
    "npl_ratio": {
      "type": "NUMBER(5,2)",
      "description": "Non-Performing Loans ratio",
      "nullable": true,
      "unit": "percentage"
    },
    "assets": {
      "type": "NUMBER(15,2)",
      "description": "Total bank assets",
      "nullable": true,
      "unit": "millions AED"
    },
    "loans": {
      "type": "NUMBER(15,2)",
      "description": "Total loans issued",
      "nullable": true,
      "unit": "millions AED"
    },
    "equity": {
      "type": "NUMBER(15,2)",
      "description": "Total equity capital",
      "nullable": true,
      "unit": "millions AED"
    }
  },
  "primary_key": ["bank"],
  "indexes": [
    {
      "name": "idx_ytd_profit",
      "columns": ["ytd_profit"],
      "type": "BTREE"
    },
    {
      "name": "idx_deposits",
      "columns": ["deposits"],
      "type": "BTREE"
    }
  ]
}