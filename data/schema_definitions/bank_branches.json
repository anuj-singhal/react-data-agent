{
  "table_name": "bank_branches",
  "description": "Bank branch locations and details across UAE",
  "ddl": "CREATE TABLE bank_branches (\n  branch_id NUMBER PRIMARY KEY,\n  bank_id VARCHAR2(100),\n  branch_name VARCHAR2(200),\n  location VARCHAR2(100),\n  emirate VARCHAR2(50),\n  branch_type VARCHAR2(50)\n)",
  "columns": {
    "branch_id": {
      "type": "NUMBER",
      "description": "Unique branch identifier",
      "nullable": false,
      "is_primary_key": true
    },
    "bank_id": {
      "type": "VARCHAR2(100)",
      "description": "Reference to parent bank",
      "nullable": false,
      "is_foreign_key": true,
      "references": "uae_banks_financial_data.bank"
    },
    "branch_name": {
      "type": "VARCHAR2(200)",
      "description": "Name of the branch",
      "nullable": true
    },
    "location": {
      "type": "VARCHAR2(100)",
      "description": "Branch location/address",
      "nullable": true
    },
    "emirate": {
      "type": "VARCHAR2(50)",
      "description": "UAE Emirate where branch is located",
      "nullable": true,
      "values": ["Abu Dhabi", "Dubai", "Sharjah", "Ajman", "Ras Al Khaimah", "Fujairah", "Umm Al Quwain"]
    },
    "branch_type": {
      "type": "VARCHAR2(50)",
      "description": "Type of branch (Main, Sub, ATM, Digital)",
      "nullable": true
    }
  },
  "primary_key": ["branch_id"],
  "indexes": [
    {
      "name": "idx_bank_id",
      "columns": ["bank_id"],
      "type": "BTREE"
    },
    {
      "name": "idx_emirate",
      "columns": ["emirate"],
      "type": "BTREE"
    }
  ]
}